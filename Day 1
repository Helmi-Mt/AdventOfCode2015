let listInput = input.split('');
function whichFLoor(listInput){
    let result = 0;
    for (let i = 0; i < listInput.length; i++) {
        if(listInput[i]=='('){
            result++;
        }else{
            result--;
        }
        
    }
    return result;
}
let floor = whichFLoor(listInput)
// part 2 : when he enters the basement
function whenBasement(listInput){
    let result = 0;
    let whenBasement;
    for (let i = 0; i < listInput.length; i++) {
        if(listInput[i]=='('){
            result++;
        }else{
            result--;
            if(result == -1){
                whenBasement = i+1;
                break;
            }
        }
        
    }
    return whenBasement;
} // 1783 the good answer
